{
  "name": "svelte-app",
  "version": "1.0.0",
  "@comments scripts": {
    "dev": "develop with blazing fast rebuilds",
    "dev:features": "develop with features like SSR and serviceworker enabled",
    "build": "run build scripts below",
    "build:app": "build single page application (SPA)",
    "build:static": "Generate static pages",
    "serve": "serve content in 'dist' folder",
    "rollup": "run the rollup bundler",
    "nollup": "run the nollup no-bundler",
    "routify": "run routify"
  },
  "scripts": {
    "gulp:clean": "gulp clean",
    "gulp:dev": "gulp",
    "gulp:prod": "gulp prod",
    "dev": "run-s build:collections gulp:dev dev:app",
    "dev:ssr": "run-s build:collections gulp:dev dev:app:ssr",
    "dev:app": "run-p cms-proxy routify nollup",
    "dev:app:ssr": "run-p cms-proxy routify rollup",
    "cms-proxy": "netlify-cms-proxy-server",
    "build": "run-s build:* gulp:prod",
    "build:app": "routify -b && rollup -c",
    "build:static": "spank",
    "build:collections": "node ./genCollections.js",
    "serve": "spassr --ssr",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify",
    "start": "sirv dist --host 0.0.0.0 --port 5000",
    "prod": "run-s build start"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^8.1.0",
    "@roxi/routify": "^2.8.5",
    "@tailwindcss/ui": "^0.6.2",
    "@tsconfig/svelte": "^1.0.10",
    "@types/node": "^14.14.20",
    "autoprefixer": "^9.8.6",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "del": "^6.0.0",
    "favicons": "^6.2.0",
    "fs-extra": "^9.0.1",
    "gray-matter": "^4.0.2",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-inject": "^5.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-string-replace": "^1.1.2",
    "gulp-webp": "^4.0.1",
    "mdsvex": "^0.8.8",
    "netlify-cms-proxy-server": "^1.3.7",
    "nollup": "^0.13.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.4",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^13.0.0",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.33.1",
    "rollup-plugin-hot": "^0.1.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.1.0",
    "rollup-plugin-svelte-hot": "^0.11.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^5.2.1",
    "sirv": "^1.0.10",
    "sirv-cli": "^1.0.6",
    "sitemap": "^6.3.2",
    "spank": "^1.4.0",
    "spassr": "^2.1.6",
    "svelte": "^3.29.4",
    "svelte-check": "^1.0.53",
    "svelte-preprocess": "^4.6.1",
    "svelte-sequential-preprocessor": "^0.0.4",
    "svelte-windicss-preprocess": "^1.0.7",
    "tailwindcss": "^1.8.13",
    "tossr": "^1.3.1",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@glidejs/glide": "^3.4.1",
    "nprogress": "^0.2.0",
    "prismjs-tomorrow-theme": "^0.0.1",
    "svelte-class-transition": "^1.0.4",
    "webfontloader": "^1.6.28"
  },
  "routify": {
    "extensions": "svelte,html,svx,md"
  },
  "spassr": {
    "assetsDir": [
      "dist",
      "assets"
    ],
    "script": "dist/build/main.js",
    "ssrOptions": {
      "inlineDynamicImports": true
    }
  },
  "spank": {
    "blacklist": [
      "/example/modal/basic/4"
    ]
  }
}
